    $oneTVSet = ( { (один/1) [@TVSet] }
        | { только в (комнат*/гостин*/зал*) }
        | { только на кухн* }
        | { (одном/1) [@TVSet] } 
        | на (одном/1) )
    
    $moreThanOneTVSet = ( { несколько [@TVSet] }
        | { (два/2) [@TVSet] }
        | { (три/3) [@TVSet] }
        | { (четыре/4) [@TVSet] }
        | { (пять/5) [@TVSet] }
