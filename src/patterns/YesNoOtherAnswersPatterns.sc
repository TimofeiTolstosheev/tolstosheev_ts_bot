        | {(@Want [стать/быть]) ([~ваш [~новый]] $client)}
        | {(@Want ([начать] $regexp<(ис)?пользовать(ся)?>)) ([~ваш] (услуг*/@Internet/@Television))} )
    
    $yesClient = ( [$yes] являюсь *
        | [$yes] являемся *
        | [$yes] ваш
        | [$yes] { я $client *}
        | [$yes] пользу* $weight<+0.5> *
